testouterr_cmd bash {switch bar/1 var=val2} $ans $tserr
testouterr_cmd bash {switch -f bar/1 var=val2} $ans $tserr

#FIXME: conflict unload mechanism should reload depre/1
set tserr [msg_unload {depre/1 <sS>} $err_superstickyunload]\n\n[msg_unload bar/1{var=val1} [err_depun depre/1]]\n\n[msg_load bar/1{var=val2} [err_conun bar/1{var=val1}]]
testouterr_cmd bash {load bar/1 var=val2} ERR $tserr
set tserr [msg_unload {depre/1 <sS>} $err_superstickyunload]\n\n[msg_unload bar/1{var=val1} [err_depunf depre/1]]\n\n[msg_load bar/1{var=val2} [err_depun depre/1]]
testouterr_cmd bash {load -f bar/1 var=val2} ERR $tserr


