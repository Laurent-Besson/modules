testouterr_cmd bash {load bar/1 var=val3} $ans $tserr
testouterr_cmd bash {load -f bar/1 var=val3} $ans $tserr


setenv_loaded_module [list bar/1 depre/1] [list $mp/bar/1 $mp/depre/1] [list bar/1]
