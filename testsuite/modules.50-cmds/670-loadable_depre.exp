lappend ans [list set LOADEDMODULES foo/1.0:bar/1:depre/1]
lappend ans [list set __MODULES_LMEXTRATAG depre/1&super-sticky]
lappend ans [list set __MODULES_LMTAG depre/1&super-sticky]
